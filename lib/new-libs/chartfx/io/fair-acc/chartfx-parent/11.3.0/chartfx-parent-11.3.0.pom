      <id>hbraeun</id>
      <name>Harald Brauening</name>
      <email>H.Braeuning@gsi.de</email>
      <url>https://www.gsi.de/en/work/project_management_fair/commons/beam_instrumentation.htm</url>
      <roles>
        <role>owner</role>
